{
  "name": "parasol",
  "version": "0.0.0",
  "description": "ES6 module for multiobjective optimization problems, based on d3 v5 and parcoords-es modules",
  "main": "dist/parasol.js",
  "module": "dist/parasol.esm.js",
  "jsnext:main": "dist/parasol.esm.js",
  "style": "dist/parcoords.css",
  "directories": {
    "lib": "lib"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "_prebuild": "eslint src",
    "_prebuild:fix": "eslint --fix src",
    "build": "rollup -c",
    "dev": "rollup -c rollup.config.dev.js -w",
    "watch": "rollup -c -w",
    "_pretest": "npm run build",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/*.js",
    "test:watch": "nyc mocha --reporter min --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "prettier --single-quote --trailing-comma es5 --write \"{src,__{demo,dist}__}/**/*.js\"",
    "prebulish": "npm run build prepare"
  },
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshhjacobson/parasol.git"
  },
  "keywords": [
    "es6",
    "d3v5",
    "Parasol",
    "parallel coordinates",
    "multiobjective optimization",
    "visualization"
  ],
  "author": "Josh Jacobson <joshuah.jacobson@gmail.com> (http://joshhjacobson.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshhjacobson/parasol/issues"
  },
  "homepage": "https://github.com/joshhjacobson/parasol#readme",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.54",
    "@babel/core": "7.0.0-beta.54",
    "@babel/plugin-external-helpers": "^7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/register": "7.0.0-beta.54",
    "babel-plugin-istanbul": "^4.1.6",
    "babelrc-rollup": "^3.0.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "istanbul": "^0.4.5",
    "livereload": "^0.7.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^4.0.0-beta.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-terser": "^1.0.1",
    "rollup-watch": "^4.3.1"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.54",
    "d3": "^5.5.0",
    "d3-dsv": "^1.0.8",
    "lodash-es": "^4.17.10",
    "ml-kmeans": "^4.1.0",
    "parcoord-es": "^2.1.7",
    "slickgrid-es6": "^3.0.1"
  }
}
